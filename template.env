# Do NOT edit template.env!
# Copy template.env to .env and edit the values there
# Use this for easier local testing

# Set Global logging level
LOG_LEVEL=INFO

# Set Slack environment parameters
SLACK_ENABLED=False
SLACK_TOKEN=
SLACK_CHANNEL_ERRORS=XXXXXXXXXXX
SLACK_CHANNEL_NEWS=XXXXXXXXXXX
SLACK_CHANNEL_CVE=XXXXXXXXXXX

# Set Zendesk environment parameters
ZENDESK_ENABLED=False
ZENDESK_TOKEN=
ZENDESK_BASE_URL=https://<zendesk_instance>.domain.tld
ZENDESK_EMAIL=<from_address>@domain.tld
ZENDESK_GROUP_ID=xxxxxxxx

# Set Notion environment parameters
NOTION_ENABLED=False
NOTION_TOKEN=
NOTION_API_VERSION=2022-02-22
NOTION_BASE_URL=https://api.notion.com/v1/"
NOTION_DB_ERRORS=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
NOTION_DB_NEWS=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
NOTION_DB_CVE=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Set Splunk environment parameters
SPLUNK_ENABLED=False
SPLUNK_API_USER=
SPLUNK_API_PASS=
SPLUNK_BASE_URL=splunk.domain.tld
SPLUNK_SCHEME=https
SPLUNK_PORT=8089
